{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "startj": "UMI_ENV=jeremy umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.1.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.1.0",
    "antd": "^4.1.1",
    "antd-mobile": "^2.3.1",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "echarts": "^4.7.0",
    "lint-staged": "^10.0.7",
    "memoize-one": "^5.1.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-container-query": "^0.11.2",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.12.0",
    "react-highlight-words": "^0.16.0",
    "react-media": "^1.10.0",
    "umi": "^3.1.0",
    "yorkie": "^2.0.0"
  }
}
